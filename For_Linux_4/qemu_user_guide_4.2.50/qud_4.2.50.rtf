\par data=$SECRET,iv=$(<iv.b64)
\par }\pard \ltrpar\ql \li720\ri0\widctlpar\wrapdefault\aspalpha\aspnum\faauto\adjustright\rin0\lin720\itap0\pararsid3036471 {\rtlch\fcs1 \af2\afs27 \ltrch\fcs0 \f2\fs27\cf1\lang1049\langfe1049\langfenp1049\insrsid3036471\charrsid3036471 -object sev-guest,id=}
